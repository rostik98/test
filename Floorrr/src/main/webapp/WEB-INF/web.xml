<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>room</servlet-name>
  	<display-name>room</display-name>
  	<description></description>
  	<servlet-class>rooms.servlets.ShowingRoomServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>addRoom</servlet-name>
  	<display-name>addRoom</display-name>
  	<description></description>
  	<servlet-class>rooms.servlets.AddingRoomServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>room</servlet-name>
  	<url-pattern>/room</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>addRoom</servlet-name>
  	<url-pattern>/addRoom</url-pattern>
  </servlet-mapping>
</web-app>
